    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "prettier": "^1.15.3",
    "lint-staged": "^4.3.0",
    "rethinkdbdash": "2.x",
    "rimraf": "^2.6.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "rethinkdbdash": "2.x"
  }
