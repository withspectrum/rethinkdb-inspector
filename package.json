{
  "name": "rethinkdb-inspector",
  "version": "0.3.3",
  "description": "Inspect your RethinkDB queries and find out how fast they are",
  "repository": {
    "type": "git",
    "url": "https://github.com/withspectrum/rethinkdb-inspector.git"
  },
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "precommit": "lint-staged",
    "prebuild": "rimraf dist",
